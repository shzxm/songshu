[Unit]
Description=SSR Node Service for SSPanel-Uim
After=ddns.service

[Service]
Type=simple
User=root
Group=root
ExecStart=/usr/bin/nohup /usr/bin/python /root/shadowsocks/server.py m>> /root/ssr_node.log 2>&1 &
ExecStop=/usr/bin/bash /soft/shadowsocks/stop.sh
Restart=always
LimitNOFILE=512000

[Install]
WantedBy=multi-user.target
